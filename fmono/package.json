{
  "apidoc": {
    "name": "fmono api",
    "version": "1.0.0"
  },
  "author": {
    "name": "George Fogle",
    "web": "https://github.com/gfogle"
  },
  "dependencies": {
    "ejs": "3.1.6",
    "fastify": "3.21.6",
    "fastify-cookie": "5.3.1",
    "htm": "3.1.0",
    "isomorphic-unfetch": "3.1.0",
    "point-of-view": "4.15.2",
    "preact": "10.5.15",
    "preact-render-to-string": "5.1.19"
  },
  "description": "Monolith Fastify ",
  "devDependencies": {
    "0x": "4.11.0",
    "@hapi/code": "8.0.3",
    "@hapi/lab": "24.3.2",
    "@types/ejs": "3.1.0",
    "@types/node": "14.14.16",
    "apidoc": "0.29.0",
    "autocannon": "7.4.0",
    "jsdoc": "3.6.7",
    "nodemon": "2.0.13",
    "prettier": "2.4.1",
    "rosie": "2.1.0",
    "testdouble": "3.16.2",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=14 <15",
    "npm": ">=6 <7"
  },
  "license": "ISC",
  "name": "@gfogle/fmono",
  "nodemonConfig": {
    "watch": [
      "src/"
    ],
    "ignore": [],
    "ext": "js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gfogle/gfogle"
  },
  "scripts": {
    "docs": "npm run docs:api && npm run docs:src",
    "docs:api": "./node_modules/.bin/apidoc -i src -o public/apidoc",
    "docs:src": "./node_modules/.bin/jsdoc -c .jsdoc.js -r -d public/jsdoc",
    "lint": "./node_modules/.bin/tsc --diagnostics --noEmit",
    "profile": "PORT=4000 ./node_modules/.bin/0x -o src/server.js",
    "perf": "PORT=4000 ./node_modules/.bin/0x -P 'autocannon localhost:4000' src/server.js",
    "test": "./node_modules/.bin/lab -r html -o test/coverage/index.html -r console -o stdout"
  },
  "version": "0.1.0"
}
